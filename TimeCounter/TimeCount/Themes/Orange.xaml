<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <FontFamily x:Key="DefaultFontFamily">SegoeUI</FontFamily>
    <Color x:Key="LightAccentColor">#F7F7F7</Color>
    <Color x:Key="2ndBackgroundColor">#E0E0E0</Color>
    <SolidColorBrush x:Key="2ndBackgroundBrush" Color="#E0E0E0" />
    <SolidColorBrush x:Key="WindowForegroundBrush" Color="Gray" />
    <SolidColorBrush x:Key="LabelBrush" Color="#454545" />
    <SolidColorBrush x:Key="HighlightBrush" Color="Black" />
    <SolidColorBrush x:Key="BackgroundBrush" Color="#EBEBEB" />
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#EE7600" />
    <SolidColorBrush x:Key="LightAccent" Color="#F7F7F7" />
    <SolidColorBrush x:Key="DarkAccent" Color="#2B2B2B" />
    <LinearGradientBrush x:Key="WindowBackgroundBrush"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0"
                         SpreadMethod="Reflect">
        <GradientStop Color="#EBEBEB"
                      Offset="0" />
        <GradientStop Color="#F5F5F5"
                      Offset="1" />
    </LinearGradientBrush>



</ResourceDictionary>